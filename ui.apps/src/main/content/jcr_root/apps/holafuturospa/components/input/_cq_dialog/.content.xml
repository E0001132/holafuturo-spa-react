<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Holafuturo form input"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[holafuturo.base]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Atributos de Propósito y Tipo (Definición Base)"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <Id
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Id"
                                                name="./id"
                                                required="{Boolean}true"/>
                                            <name
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Nombre"
                                                name="./name"/>
                                            <type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                defaultValue="text"
                                                fieldLabel="Tipo"
                                                name="./type">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <text
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Input de texto"
                                                        value="text"/>
                                                    <email
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Input mail"
                                                        value="email"/>
                                                    <number
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Input numerico"
                                                        value="number"/>
                                                    <tel
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Input teléfono"
                                                        value="tel"/>
                                                </items>
                                            </type>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <validation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Atributos de Validación y Restricción"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <required
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./required"
                                                text="¿Es obligatorio?"
                                                value="true"/>
                                            <minlength
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Tamaño minimo permitido"
                                                name="./minlength"
                                                required="{Boolean}false"/>
                                            <maxlength
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Tamaño máximo permitido"
                                                name="./maxlength"
                                                required="{Boolean}false"/>
                                            <min
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Min"
                                                name="./min"
                                                required="{Boolean}false"/>
                                            <max
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Max"
                                                name="./max"
                                                required="{Boolean}false"/>
                                            <step
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Step"
                                                name="./step"
                                                required="{Boolean}false"/>
                                            <pattern
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Pattern"
                                                name="./pattern"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </validation>
                    <AparienciaAccesibilidad
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Atributos de Apariencia y Accesibilidad (UX/UI)"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Placeholder"
                                                name="./placeholder"/>
                                            <value
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Value"
                                                name="./value"/>
                                            <disabled
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./disabled"
                                                text="¿Esta deshabilitado?"
                                                value="true"/>
                                            <checked
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./checked"
                                                text="¿Está check?"
                                                value="true"/>
                                            <readonly
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./readonly"
                                                text="¿es de solo lectura?"
                                                value="true"/>
                                            <class
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Clase"
                                                name="./class"/>
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Etiqueta"
                                                name="./label"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                        <comportamientoNavegador
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                            <items jcr:primaryType="nt:unstructured">
                                <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <list
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    defaultValue="text"
                                                    fieldLabel="List"
                                                    name="./list">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <text
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Texto"
                                                            value="text"/>
                                                        <email
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Email"
                                                            value="email"/>
                                                        <search
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Search"
                                                            value="search"/>
                                                        <tel
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Telefono"
                                                            value="tel"/>
                                                        <url
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Url"
                                                            value="url"/>
                                                    </items>
                                                </list>
                                            </items>
                                        </column>
                                    </items>
                                </columns>
                            </items>
                        </comportamientoNavegador>
                    </AparienciaAccesibilidad>
                    <comportamientoFormulario
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Atributos de Comportamiento del Formulario y Asociación."
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <form
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Form"
                                                name="./form"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </comportamientoFormulario>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
